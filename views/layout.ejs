    <script src="/js/dependencies/sails.io.js"></script>
    <script src="/js/dependencies/lib/1.11.1.jquery.min.js"></script>
    <script src="/js/dependencies/lib/angular.min.js"></script>
    <script src="/js/dependencies/lib/bootstrap.min.js"></script>
    <script src="/js/interact-1.2.4.min.js"></script>
    <script src="/js/jPushMenu.js"></script>
    <script src="/js/jquery.validate.min.js"></script>
    <script src="/js/lightbox.js"></script>
    <script src="/js/scripts.js"></script>
    <script src="/js/timeline_modernizr.js"></script>
    <script src="/js/timline.js"></script>
